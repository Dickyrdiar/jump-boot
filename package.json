{
    "name": "jump-boot",
    "description": "",
    "version": "1.0.0",
    "main": "./jump-boot/start.js",
    "license": "MIT",
    "type": "commonjs",
    "repository": {
        "url": "git@github.com:jefripunza/jump-boot.git"
    },
    "scripts": {
        "start": "node .",
        "dev": "npx nodemon",
        "format": "npm run format:prettier && npx eslint \"**/*.js\" --ignore-path ./.prettierignore --fix",
        "format:prettier": "prettier \"**/*.js\" --ignore-path ./.prettierignore --write",
        "pm2:add": "npx pm2 start ./jump-boot/start.js --name service_name",
        "pm2:start": "npx pm2 start service_name",
        "pm2:stop": "npx pm2 stop service_name",
        "pm2:delete": "npx pm2 delete service_name",
        "version": "node -p \"require('./package.json').version\"",
        "test": "mocha tests/*.js --exit"
    },
    "_moduleAliases": {
        "@root": ".",
        "@jump-boot": "jump-boot",
        "@controllers": "controllers",
        "@middleware": "middleware",
        "@entities": "models/entities",
        "@repository": "models/repository",
        "@services": "services"
    },
    "lint-staged": {
        "js-framework/*.{js,json}": [
            "prettier --ignore-path ./.prettierignore --write"
        ]
    },
    "dependencies": {
        "@adiwajshing/baileys": "^4.0.0",
        "body-parser": "^1.19.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-fileupload": "^1.3.1",
        "extract-zip": "^2.0.1",
        "fs-extra": "^10.0.1",
        "google-tts-api": "^2.0.2",
        "helmet": "^5.0.2",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "mysql2": "^2.3.3",
        "node-emoji": "^1.11.0",
        "node-fetch": "^3.2.0",
        "nodemailer": "^6.7.2",
        "pg": "^8.7.3",
        "pino-pretty": "^7.5.1",
        "qrcode": "^1.5.0",
        "qrcode-terminal": "^0.12.0",
        "socket.io": "^4.4.1",
        "typeorm": "^0.2.42"
    },
    "devDependencies": {
        "chai": "^4.3.6",
        "chai-http": "^4.3.0",
        "chalk": "2.4.1",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "figlet": "^1.5.2",
        "mocha": "^9.2.1",
        "nodemon": "^2.0.15",
        "pm2": "^5.1.2",
        "prettier": "^2.5.1",
        "yargs": "^17.3.1"
    }
}
